{
  "expo": {
    "name": "Parkpal",
    "slug": "parkpal",
    "description": "Airbnb but for parkings.",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon-parkpal.png",
    "splash": {
      "image": "./assets/parkpal-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-parkpal.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": true
        }
      ]
    ]
  }
}
